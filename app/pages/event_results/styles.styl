.gc-event-results
  height: calc(100% - 80px) !important

  .gc-nav-wrapper
    display: flex
    justify-content: space-between
    align-items: center
    padding-bottom: 9px

    > div
      display: flex
      align-items: center

      &.text-sm-right
        justify-content: flex-end

    .name
      margin-left: 10px
      color: #9ea0a1
      font-size: 16px
      text-transform: uppercase

    .date
      padding-left: 28px
      color: #3a99d8
      background: url('../../images/calendar.png') no-repeat
      background-size: 18px

    nav
      margin-left: 10px

      .pagination
        margin: 0
