*
  margin: 0

html,
body
  height: 100%

body
  background: #282d30
  font-family: 'Source Sans Pro', sans-serif

body.dragging,
body.dragging *
  cursor: move !important

.gc-centered
  margin-left: auto
  margin-right: auto
  float: none

.gc-fixed-top-bar .gc-topbar
  position: fixed
  top: 0
  z-index: 10
  left: 230px
  right: 0
  min-width: 500px
  max-width: 100%
  width: auto
  display: block

.gc-fixed-top-bar .gc-topbar > div
  display: table
  width: 100%

.gc-main-workspace
