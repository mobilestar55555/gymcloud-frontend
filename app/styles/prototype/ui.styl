.gc-checkbox
.gc-radio
  position relative

.gc-checkbox input[type="checkbox"]
.gc-radio input[type="radio"]
  visibility hidden

.gc-checkbox input[type="checkbox"] + .gc-checkbox-clone
  height 14px
  width 14px
  display inline-block
  border 1px solid #ccc
  position absolute
  left -1px
  top 3px
  border-radius 3px
  background #fff
  outline none

.gc-checkbox input[type="checkbox"]:checked + .gc-checkbox-clone
  background-image url('../../images/prototypes/checkbox-icon.png')
  background-position center
  background-repeat no-repeat

.gc-checkbox input[type="checkbox"]:not(:checked):not(:disabled):hover + .gc-checkbox-clone:after
  margin-top 1px
  margin-left 1px
  height 10px
  width 10px
  content ''
  display block
  background-image url('../../images/prototypes/checkbox-icon.png')
  background-position center
  background-repeat no-repeat
  opacity 0.5

.gc-radio input[type="radio"]:disabled + .gc-radio-clone
.gc-checkbox input[type="checkbox"]:disabled + .gc-checkbox-clone
.gc-radio input[type="radio"]:disabled + .gc-radio-clone:after
  background-color #efefef!important

.gc-radio input[type="radio"] + .gc-radio-clone
  height 14px
  width 14px
  display inline-block
  border 1px solid #ccc
  position absolute
  left -1px
  top 3px
  border-radius 100%
  background #fff
  outline none

.gc-radio input[type="radio"]:not(:checked):not(:disabled):hover + .gc-radio-clone:after
  margin-top 2px
  margin-left 2px
  height 8px
  width 8px
  content ''
  display block
  opacity 0.5
  background blue
  border-radius 100%

.gc-radio input[type="radio"]:checked + .gc-radio-clone
  padding 2px

.gc-radio input[type="radio"]:checked + .gc-radio-clone:after
  display block
  height 100%
  width 100%
  background blue
  content ''
  border-radius 100%

