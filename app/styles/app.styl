[contenteditable]
  -webkit-user-select: text
  user-select: text

  &:empty:before
    content: attr(placeholder)
    display: block

.gc-box-content
  padding: 5px 15px

.gc-calendar-activity
  margin: 0
  padding: 15px 0 0 0

  li
    margin: 0 0 10px 0
    list-style-type: none
    position: relative

li.gu-mirror
  list-style-type none

body
  > li.gu-mirror
    a
      text-transform: uppercase

.gc-folder .gu-transit
  display none

.gc-folder.gc-folder-droppable-state
  border 1px dashed #3a99d8!important
  background-color rgba(58,153,216,0.2)

.gc-assign-video-upload,
.gc-manage-video-upload
  margin-top: 10px

  .gc-upload-video
    margin 2px 0px
    padding 5px 20px
    i.gc-icon
      height: 15px;
      width: 20px;
      float: left;
      margin-top: 1px;

.gc-assign-video-filter
  border-bottom none!important

.gc-main-workspace

  .gc-workspace-content
    // position: absolute
    top: 50px
    bottom: 0
    left: 0
    right: 0
    overflow-y: auto
    overflow-x: hidden
    height: calc(100% - 50px)
    width: 100%

    &.personal-programs
      height: calc(100% - 120px)

.gc-client-list
  height: auto
  position: absolute
  overflow: auto
  top: 184px
  bottom: 0
  left: 0
  right: 0

.gc-clientpage-exercises
  height: 100%
  max-height: 100%
  overflow-x: visible
  overflow-y: auto
  margin-left 0

#client-header-region
  padding: 0 15px

.draggable-handle
  cursor: move

[data-balloon]
  &:after
    text-transform: none
    line-height: normal
    font-style: normal
    font-weight: normal

.visible
  display: block