  position: relative
  height: 100%

.gc-content-both
  /*float: none;*/

.gc-content-both > .row
  overflow: hidden

.gc-content-left
  border-right: 1px solid rgba(40,45,48,0.1)
  height: 100%
  padding: 0

.gc-content-right
  height: inherit
  padding-bottom: 12%
  overflow-y: auto

.gc-content-right header
  height: 50px
  border-bottom: 1px solid rgba(40,45,48,0.1)

.gc-content-right header h3
  font-size: 11px
  text-transform: uppercase
  color: rgba(158, 160, 161, 0.5)
  text-align: center
  line-height: 15px
  margin: 0
  height: 50px
  padding: 10px 0

@media (min-width: 1025px)
  .gc-content-right header h3
    font-size: 18px
    text-align: center
    line-height: 50px
    margin: 0
    padding: 0

.gc-box-content .gc-nav-wrapper
  height: auto
  margin: -5px -15px 0 -15px
  padding-bottom: 9px
  background-color: #F9F9F9
  z-index: 3

.gc-nav-wrapper
  min-height: 50px
  padding: 9px 15px 0 0
  margin-left: 0
  border-bottom: 1px solid rgba(40,45,48,0.1)
  overflow: visible
  position: relative
  z-index: 4

  & + .gc-nav-wrapper
    height: auto
    padding-bottom: 9px
    background-color: #F9F9F9
    z-index: 3

  &.without-shadow
    box-shadow: none

  .gc-col-1 > *
    float: left

  .gc-content-nav-back
    margin: 6px 5px
    display block
    float: left
    color: #337ab7
    cursor: pointer

  .gc-content-breadcrumbs
    display block
    overflow: hidden

  h2
    display: inline-block
    text-transform: uppercase
    font-size: 19px
    text-transform: uppercase
    color: #9ea0a1
    padding: 0
    margin: 0
    font-weight: bold
    color: #757575
    line-height: 31px
    margin-left: 0.9rem

  .btn
    margin-left: 1rem

.gc-nav-right-side
  position: absolute
  right: 15px
  bottom: -1px

.gc-nav-shadow
  box-shadow: 0px 5px 10px -8px rgba(143,143,143,1)

.gc-content-both > .row.gc-nav-wrapper
  overflow: visible

.gc-nav-wrapper .btn,
