.gc-exercises-add-best
  text-align: center
  margin-top: 15px

.gc-exercises-add-best a
  font-size: 10px
  color: #3a99d8
  display: inline-block
  text-transform: uppercase
  border: 1px solid #3a99d8
  border-radius: 50px
  line-height: 10px
  padding: 2px 4px
  margin: 0 3px
  cursor: pointer

.gc-exercises-add-best a:active,
.gc-exercises-add-best a:focus
  color: #3a99d8

.gc-exercises-add-best a:hover
  background-color: #3a99d8
  color: #fff
  text-decoration: none

.gc-exercises-no-best
  background: transparent url('../images/icon-cup-large.png') center 10px no-repeat
  text-align: center
  background-size: 40%
  padding: 60px 10px 0px 10px
  color: #cbcacb
  font-size: 12px
  margin-bottom: 5px

.gc-exercises-bests
  padding: 0
  margin-top: 20px

.gc-exercises-bests-best
  list-style-type: none
  margin-bottom: 15px

.gc-exercises-bests-best > .row > div
  padding: 0 5px

.gc-exercises-bests-best h6
  color: #3a99d8
  font-size: 14px
  margin-top: 3px

.gc-exercises-bests-icon
  border: 1px solid #3a99d8
  border-radius: 50px
  text-align: center
  height: 20px
  width: 20px
  display: inline-block
  margin: 0 0 -6px -6px

.gc-exercises-bests-icon img
  height: 10px
  margin-top: -3px

.gc-exercises-bests-best > .row
  margin: 0 0 1px 0

.gc-exercises-bests-result
  color: #404447
  font-size: 13px
  text-transform: uppercase

.gc-exercises-bests-result > div:first-child
  color: #9ea0a1

.gc-add-personal-best-modal .modal-content
  border-radius: 0

.gc-add-personal-best-modal .modal-header
  background: #3a99d8

.gc-add-personal-best-modal .modal-header h4
  font-size: 14px
  color: #fff

.gc-add-personal-best-modal .modal-header h4 img
  height: 12px
  margin-top: -4px

.gc-add-personal-best-modal .modal-header .close
  font-size: 10px
  font-weight: normal
  color: #fff
  text-shadow: none
  opacity: 0.5
  margin: 5px 10px 0 0

.gc-add-personal-best-modal .modal-footer
  border: none
  padding-top: 8px

.gc-add-personal-best-modal .modal-body
  border: none
  padding-bottom: 0

.gc-add-personal-best-date .input-group-addon
  background: #fff
  font-size: 18px
  color: #3a99d8
  border-color: #cbd4d7

.gc-add-personal-best-date .input-group-addon .fa + .fa
  margin-left: 3px

.gc-add-personal-best-property input[type=text]

.gc-add-personal-best-property input[type=text]:focus
  box-shadow: none

.gc-add-personal-best-property input[type=text].gc-invalid
  border-color: #ff560a
  box-shadow: none

.gc-add-personal-best-date.gc-invalid input,
.gc-add-personal-best-date.gc-invalid .input-group-addon
  border-color: #ff560a

.gc-add-personal-best-property > div
  padding: 0

.gc-add-personal-best-property > div:first-child
  padding: 0 5px 0 15px

.gc-add-personal-best-property > div:last-child
  padding: 0 15px 0 5px

.gc-add-personal-best-modal .form-group label
  color: #3a99d8
  font-size: 12px
  margin-bottom: 1px

.gc-add-personal-best-property .btn
  background: #fff
  color: #3a99d8
  border-color: #3a99d8
  text-transform: uppercase
  font-size: 14px
  line-height: 20px

.gc-add-personal-best-list
  padding: 0

.gc-add-personal-best-list > li
  list-style-type: none
  font-size: 15px
  margin: 0

.gc-add-personal-best-list > li:first-child
  padding-top: 15px
  border-top: 1px solid #e9eaea

.gc-add-personal-best-list > li:last-child
  padding-bottom: 15px
  border-bottom: 1px solid #e9eaea

.gc-add-personal-best-list > li + li
  margin-top: 25px

.gc-add-personal-best-list > li > div
  padding: 0

.gc-add-personal-best-list > li > div:first-child
  color: #b0b1b2
  text-transform: uppercase

.gc-add-personal-best-list > li > div > .btn
  color: #3a99d8
  padding: 0 6px
  text-transform: uppercase
  font-size: 14px
  cursor: pointer

.gc-add-personal-best-list > li > div > .btn:hover,
.gc-add-personal-best-list > li > div > .btn:focus,
.gc-add-personal-best-list > li > div > .btn:active
  background: #fff

.gc-add-personal-best-modal .modal-footer .btn
  font-size: 12px
  color: #3a99d8
  text-transform: uppercase

.gc-add-personal-best-modal .modal-footer .btn-primary
  background: #fff
  border-color: #3a99d8
  border-radius: 50px

.gc-exercises-assign
  position: relative
  min-height: 100%
  height: 100%

  .gc-folders-entity-list
    padding: 0 20px

.gc-exercises-assign .gc-exercises-assign-input-wrapper
  margin: 25px 0 10px 0
  padding: 0 20px

.gc-quick-search-wrapper
  padding: 15px

.gc-exercises-assign-input

  &::placeholder
    color: #c6c6c6
    opacity: 1

  .selectize-input.focus
    box-shadow: none
    border-color: #ccc
    border-radius: 4px 4px 0 0

  .selectize-dropdown
    border-radius: 0 0 4px 4px
    top: 31px !important
    border-color: #ccc

  .selectize-dropdown-content > div
    color: #3a99d8
    text-transform: uppercase
    padding: 6px 12px

    &:hover,
    &.active
      background-color: #f2fbff

    .label
      color: #ccc

    &:hover .label
      color: #666

    span.icon
      display: inline-block
      position: relative
      width: 13px
      height: 13px
      margin: 0px 10px -1px 0
      background-position: center center

    span.avatar
      border-radius: 20px
      display: inline-block
      height: 20px
      width: 20px
      vertical-align: -6px
      margin-right: 8px
      background: green url('../images/icon-individual-right.png') center top no-repeat
      background-size: contain

      img
        border-radius: 20px
        display: block
        height: 20px
        width: 20px

        &[src=''],
        &[src='undefined']
          display: none

.gc-exercises-chb
  display: inline-block
  padding: 5px
  margin: 0 0 0 -1px
  position: relative
  top: 1px
  cursor: pointer
  width: 25px
  height: 25px

.gc-exercises-chb input
  margin: 0
  cursor: pointer

.gc-exercises-update-time
  font-size: 14px
  line-height: 30px
  color: #404447
  display: inline-block

.gc-exercises-assign-btn
  &,
  &:focus
    background: #fff
    color: #3a99d8
    border-color: #3a99d8
    text-transform: uppercase
    font-size: 12px
    line-height: 20px
    border-radius: 50px
    padding: 4px 8px

  &:hover,
  &:active
    background: #3a99d8
    color: #fff
    border-color: #fff

  + .btn
    margin: 0 15px


.gc-exercises-assign-btn-del
  font-size: 18px
  color: rgba(40, 45, 48, 0.4)
  bottom: -2px
  position: relative
  display: inline-block

  &:hover,
  &:active,
  &:focus
    color: #ee6d5d

  .gc-icon-trash
    height: 16px

.gc-exercises-assign-empty
  margin: 15% auto 0 auto
  width: 170px
  height: 180px
  text-align: center

  img
    opacity: 0.25
    margin-bottom: 20px

  p
    font-size: 12px
    color: #cbcacb
    line-height: 15px

  .btn
    background: #fff
    color: #3a99d8
    border-color: #3a99d8
    text-transform: uppercase
    font-size: 12px
    line-height: 20px
    border-radius: 50px
    padding: 4px 8px

    &:active,
    &:focus
      color: #3a99d8
      border-color: #3a99d8

    &:hover
      color: #fff
      border-color: #3a99d8
      background: #3a99d8
      text-decoration: none

.gc-search-results-main
  .gc-exercises-assign-empty
    margin-top: 20%

.gc-exercises-item-icon
  display: inline-block
  background: transparent url('../images/icon-exercises.png') right -2px no-repeat
  height: 25px
  width: 23px
  background-size: cover
  vertical-align: middle
  margin: 0 5px 0 4px

  &.gc-exercises-item-icon-programs,
  &.gc-exercises-item-icon-program_templates
    background: transparent url('../images/icon-programs.png') -19px 1px no-repeat
    height: 25px
    width: 23px
    background-size: 154%

  &.gc-exercises-item-icon-workouts,
  &.gc-exercises-item-icon-workout_templates
    background: transparent url('../images/icon-workouts.png') -18px 0px no-repeat
    height: 25px
    width: 23px
    background-size: 154%

  &.gc-exercises-item-icon-warmups,
  &.gc-exercises-item-icon-warmups_templates
    background: transparent url('../images/icon-warmups.svg') 1px 3px no-repeat
    height: 25px
    width: 22px
    background-size: 80%

  &.gc-exercises-item-icon-cooldowns,
  &.gc-exercises-item-icon-cooldowns_templates
    background: transparent url('../images/icon-cooldowns.svg') 1px 3px no-repeat
    height: 25px
    width: 22px
    background-size: 80%

.gc-exercises-item-play-icon
  display: inline-block
  background: transparent url('../images/play-button.png') right 0 no-repeat
  height: 25px
  width: 23px
  background-size: cover
  vertical-align: middle
  margin: 0 5px 0 4px

.gc-check-icon
  display: inline-block
  background: transparent url('../images/icon-check.png') right 0 no-repeat
  height: 25px
  width: 23px
  background-size: cover
  vertical-align: middle
  margin: 0 5px 0 4px

.gc-exercises-folder-icon
  color: #3a99d8
  display: inline-block
  margin: 0 9px 0 7px
  font-size: 16px

.gc-content-right-exercises-empty
  margin: 50% auto 0 auto
  width: 170px
  height: 180px
  text-align: center

  img
    opacity: 0.25
    margin-bottom: 20px

  p
    font-size: 14px
    color: #cbcacb
    line-height: 16px

.selectize-control.multi .selectize-input > div
  background: #e1eef6

  .remove
    color: #3a99d8
    font-size: 16px
    font-weight: lighter

    &:active
      color: #fff

.gc-exercises-bests-best > .row > .gc-exercises-bests-delete-wrapper
  padding: 0 5px

.gc-exercises-bests-delete
  color: #ee6d5d
  background: transparent
  border: none
  outline: none
  font-size: 16px
  height: 17px
  width: 17px
  line-height: 17px
  padding: 0
  text-align: center
  display: none

.gc-exercises-bests-best:hover .gc-exercises-bests-delete
  display: inline-block

.gc-exercises-bests-delete:hover,
.gc-exercises-bests-delete:active,
.gc-exercises-bests-delete:focus
  background: transparent
  color: #ee6d5d

.bootstrap-datetimepicker-widget td.active,
.bootstrap-datetimepicker-widget td.active:hover
  background-color: #3a99d8

.bootstrap-datetimepicker-widget thead tr:first-child th,
.bootstrap-datetimepicker-widget .picker-switch td span
  color: #3a99d8

.bootstrap-datetimepicker-widget td span
  height: 32px
  width: 32px
  line-height: 32px

.bootstrap-datetimepicker-widget td span:hover,
.bootstrap-datetimepicker-widget td span:active
  background: transparent

.gc-duplicate-form .modal-body > label
  color: #9f9f9f
  font-size: 15px

.gc-duplicate-folders-list
  max-height: 250px
  overflow-x: visible
  overflow-y: auto
  border-bottom: 1px solid #eaeaea
  list-style-type: none
  padding: 15px 0 0 0
  .radio
    margin: 0 0 0 1px

.gc-duplicate-folders-list li
  padding: 4px 0
  font-size: 15px

.gc-duplicate-folders-list li label
  color: #333

.gc-duplicate-folders-list li input
  position: relative
  margin-right: 10px
  bottom: -2px

@media (min-width: 1200px)
  .gc-exercises-no-best
    background-position: center 5px
    background-size: 18%
    padding: 70px 40px 10px 40px
    margin-bottom: 10px
    font-size: 12px

@media (min-width: 1440px)
  .gc-exercises-bests-icon
    border: 1px solid #3a99d8
    border-radius: 50px
    text-align: center
    height: 25px
    width: 25px
    display: inline-block
    margin: 0

  .gc-exercises-bests-icon img
    height: 10px
    margin: 0

  .gc-exercises-bests-best > .row > div
    padding: 0 15px

  .gc-exercises-bests-best > .row > div:first-child
    padding: 0 5px

  .gc-exercises-bests-best h6
    margin-left: -10px
    font-size: 16px

@media (min-width: 992px)
  .gc-exercises-bests
    padding: 0 10px

  .gc-exercises-bests-best > .row
    margin: 0 0 10px 0

  .gc-exercises-bests-best
    margin: 0

  .gc-exercises-add-best a
    line-height: 20px
    padding: 3px 12px
    margin: 0
    font-size: 12px

.load-more
  color: #3a99d8
  display: inline-block
  text-transform: uppercase
  border: 1px solid #3a99d8
  border-radius: 50px
  cursor: pointer
  line-height: 20px
  padding: 3px 12px
  margin: 0
  font-size: 12px

.load-more:active,
.load-more:focus
  color: #3a99d8

.load-more:hover
  background-color: #3a99d8
  color: #fff
  text-decoration: none

region.gc-videos .row
  padding: 20px 5px
  margin-left: 0px
  margin-right: 0px

.gc-assign-video-layout,
.gc-manage-video-layout
  padding: 0 15px

