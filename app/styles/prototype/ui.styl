  .gc-video-time-icon
    height 12px
    background-image url("../../images/icon-video-time.png")

  .gc-video-library-icon
    width 14px
    background-image url("../../images/icon-video-library.png")

  .gc-video-youtube-icon
    width 17px
    background-image url("../../images/icon-video-youtube.png")

  .gc-video-vimeo-icon
