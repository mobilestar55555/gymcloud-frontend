/*color constants*/
white = #ffffff
blue = #3a99d8
blueSecondary = #1d74ae
gray = #cccccc
lighGray = fafafa
black = #2f3539
blackSecondary = #282d30

green = #39ca74
orange = #ff9702
red = #e54d42

/*functions*/
transition(type, duration, animation)
  transition type duration animation

/*base styles*/
backgroundCenter =
  background-repeat no-repeat
  background-position center

faIcon =
  display block
  position absolute
  bottom 13px
  right 10px
  font-size 14px
  display inline-block
  font normal normal normal 14px/1 FontAwesome
  font-size inherit
  text-rendering auto
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
  transform translate(0, 0)

